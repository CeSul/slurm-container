echo 
echo "Building example plugin"
echo "-------------------------------------------------------------"

make plugin

echo 
echo 
echo "Submitting example job"
echo "-------------------------------------------------------------"

sbatch --wait example.job

echo 
echo 
echo "Example output"
echo "-------------------------------------------------------------"

cat example.out

echo 
echo 
echo "Building MPI example plugin"
echo "-------------------------------------------------------------"

make mpiex

echo 
echo 
echo "Submitting MPI example job"
echo "-------------------------------------------------------------"

sbatch --wait mpi_example.job

echo 
echo 
echo "Example MPI output"
echo "-------------------------------------------------------------"

cat mpi_example.out

